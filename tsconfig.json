{
  "compilerOptions": {
    "outDir": "./public/",
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@stores/*": ["src/stores/*"],
      "@styles/*": ["src/styles/*"],
      "@helpers/*": ["src/helpers/*"],
      "@pages/*": ["src/pages/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}